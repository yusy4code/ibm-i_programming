H                                      
D i               S              2P 0  
 *                                     
 /Free                                 
    // Display number 1 to 9          
    i = 1;                             
    DoU (i>=10);                       
      Dsply  i;                        
      i += 1;                          
    EndDo;                             
    *Inlr = *On;                       
 /End-Free                             
